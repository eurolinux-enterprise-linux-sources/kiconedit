msgstr "100%"

#: kiconedit.cpp:345
msgid "200%"
msgstr "200%"

#: kiconedit.cpp:351
msgid "500%"
msgstr "500%"

#: kiconedit.cpp:357
msgid "1000%"
msgstr "1000%"

